{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "lib": ["es6", "ESNext", "ESNext.Array", "ESNext.AsyncIterable", "ESNext.Intl", "ESNext.Symbol", "DOM"],
    "sourceMap": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "incremental": true,
    "allowJs": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "removeComments": true,
    "typeRoots": [
      "./node_modules/@types",
      "./src/@types"
    ],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@commands/*": ["./src/commands/*"],
      "@models/*": ["./src/models/*"],
      "@reactions/*": ["./src/reactions/*"],
      "@config/*": ["./src/config/*"],
      "@customTypes/*": ["./src/@types/*"],
      "@database": ["./src/database"],
      "@assets/*": ["./src/assets/*"],
      "@utils/*": ["./src/utils/*"]
    }
  },
  "include": [
    "src/**/*"
  ]
}